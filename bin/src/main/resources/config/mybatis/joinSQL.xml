<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="joinSQL">
	<insert id="user_add" parameterType="com.bin.bin.join.joinDTO">
		insert into User 
		values(#{userId},#{userPwd})
	</insert>
	<insert id="business_add" parameterType="com.bin.bin.join.joinDTO">
		insert into Business 
		values(#{businessId},#{businessPwd})
	</insert>
</mapper>